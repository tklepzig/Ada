    "build": "rm -rf css && sass scss:css -s compressed --no-source-map"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tklepzig/Ada.git"
  },
  "author": "Thomas Klepzig",
  "bugs": {
    "url": "https://github.com/tklepzig/Ada/issues"
  },
  "devDependencies": {
    "alive-server": "^1.2.9",
    "sass": "^1.42.1"
  }
}
